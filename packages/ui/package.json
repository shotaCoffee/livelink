{
  "name": "@band-setlist/ui",
  "version": "1.0.0",
  "description": "Shared UI components for LiveLink application",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
